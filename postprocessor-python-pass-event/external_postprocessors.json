      "SocketPath": "/tmp/python-pass-event-postprocessor.sock",
      "ReceiveInputTensor": false,
      "ReceiveConfidenceData": false,
      "Settings": [
        {
          "type": "Repeater",
          "count": 10,
          "startIndex": 0,
          "template": {
            "type": "GroupBox",
